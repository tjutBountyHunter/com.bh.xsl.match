<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xsl.match.mapper.XslMatchRankMapper" >
  <resultMap id="BaseResultMap" type="com.xsl.pojo.XslMatchRank" >
    <!--<id column="Id" property="id" jdbcType="INTEGER" />-->
    <result column="matchRankId" property="matchRankId" jdbcType="VARCHAR"/>
    <result column="rankName" property="rankName" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    matchRankId, rankName
  </sql>
  
  <select id="selectAll" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"/> from xsl_match_rank
  </select>

  <select id="selectByRankId" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"/>
    from xsl_match_rank
    where #{rankId}
  </select>

</mapper>