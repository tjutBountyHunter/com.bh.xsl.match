<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xsl.match.mapper.XslOrientedMapper" >
  <resultMap id="BaseResultMap" type="com.xsl.pojo.XslOriented" >
    <!--<id column="Id" property="id" jdbcType="INTEGER" />-->
    <result column="orientedId" property="orientedId" jdbcType="VARCHAR" />
    <result column="orientedName" property="orientedName" jdbcType="VARCHAR" />
    <result column="orientedInfo" property="orientedInfo" jdbcType="VARCHAR" />
  </resultMap>

  <sql id="Base_Column_List" >
     orientedId, orientedName, orientedInfo
  </sql>

  <select id="selectAll" resultMap="BaseResultMap">
    select <include refid="Base_Column_List"/>
    from xsl_oriented
  </select>

  <insert id="insert" parameterType="com.xsl.pojo.XslOriented">
    insert into xsl_oriented (orientedId, orientedName, orientedInfo)
    values (#{orientedId},#{orientedName},#{orientedInfo})
  </insert>

  <delete id="deleteByOrientedId" parameterType="java.lang.String">
    delete from xsl_oriented
    where orientedId = #{orientedId}
  </delete>

  <update id="updateByOrientedId" parameterType="com.xsl.pojo.XslOriented">
    update xsl_oriented
    set orientedName = #{orientedName},
     orientedInfo = #{orientedInfo}
    where orientedId = #{orientedId}
  </update>

</mapper>